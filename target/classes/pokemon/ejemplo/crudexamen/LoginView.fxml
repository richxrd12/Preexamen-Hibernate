   <children>
      <AnchorPane prefHeight="300.0" prefWidth="400.0">
         <children>
            <TextField fx:id="usuarioTextField" layoutX="29.0" layoutY="77.0" prefHeight="25.0" prefWidth="314.0" promptText="Usuario" />
            <Label layoutX="31.0" layoutY="60.0" text="Usuario" />
            <Label layoutX="31.0" layoutY="126.0" text="Contraseña" />
            <Button layoutX="165.0" layoutY="202.0" mnemonicParsing="false" onAction="#onClickLogin" text="Login" />
            <PasswordField fx:id="passwordTextField" layoutX="31.0" layoutY="143.0" prefHeight="25.0" prefWidth="314.0" promptText="Contraseña" />
         </children>
      </AnchorPane>
   </children>
